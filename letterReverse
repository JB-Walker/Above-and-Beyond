function spinWords(originalString){
  const words = originalString.split(' ');
  //const words = originalString.split(' ');
  //let spunWords = words.map(word => {
  let spunWords = words.map(word => {

     if (word.length < 5) {
      return word;
    } else {
      return word.split('').reverse().join('');
    }
  });
  return spunWords.join(' ');
}